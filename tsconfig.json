{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitReturns": true,
        "removeComments": true,
        "resolveJsonModule": true,
        "strictFunctionTypes": true
    },
    "exclude": [
        "node_modules"
    ]
}